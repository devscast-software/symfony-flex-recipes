                        "    <twig:Dashlite:Layout:Header :title=\"_title\" backTitle=\"Toutes les entr\u00e9es\" backPath=\"{{ _index_url }}\">",
                        "        <twig:block name=\"content\">",
                        "            {% if _show_url %}",
                        "                <twig:Dashlite:Button:HeaderLink name=\"d\u00e9tails\" icon=\"eye\" path=\"{{ _show_url }}\" />",
                        "            {% endif %}",
                        "        </twig:block>",
                        "    </twig:Dashlite:Layout:Header>",
                        "</twig:block>",
                        "",
