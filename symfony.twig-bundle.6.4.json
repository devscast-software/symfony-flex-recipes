                        "{% extends '@DevscastDashlite/layout/base.html.twig' %}",
                        "{% set _title = (data is defined and data) ? \"\u00c9dition #{ _entity | trans([], _domain) }\" : \"Cr\u00e9ation #{_entity | trans([], _domain)}\" %}",
